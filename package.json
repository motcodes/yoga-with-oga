  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-native": "^0.67.2",
    "eslint": "^8.11.0",
    "turbo": "^1.1.6",
